{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Debug"
        }
    },
    "AllowedHosts": "*",
    "Cors": {
        "AllowedOrigins": [
            "https://localhost:3000"
        ]
    },
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http1AndHttp2"
        },
        "Certificates": {
            "Default": {
                "Path": "../../../../certificates/aspnetapp.pfx"
            }
        }
    },
    "Secrets": {
        "SecretId": "auth-sample-secrets-development"
    }
}

