{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "ApplicationLogging": {
        "ServiceName": "Auth",
        "AddSemanticKernelInstrumentation": true,
        "EnableSemanticKernelSensitiveDiagnostics": true,
        "AddAwsInstrumentation": true
    },
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        },
        "Certificates": {
            "Default": {
                "Path": "/https/aspnetapp.pfx"
            }
        }
    },
    "Secrets": {
        "SecretId": "auth-sample-secrets-prod",
        "Prefix": "Auth_"
    }
}
