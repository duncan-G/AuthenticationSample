{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Debug"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "Certificates": {
            "Default": {
                "Path": "../../../../certificates/aspnetapp.pfx"
            }
        }
    },
    "Secrets": {
        "SecretId": "auth-sample-secrets-development"
    },
    "Redis": {
        "Configuration": "localhost:6379",
        "InstanceName": "AuthSampleCache"
    },
    "DynamoDb": {
        "ServiceUrl": "http://localhost:8000",
        "TimeoutSeconds": 3,
        "MaxErrorRetry": 0
    },
    "Cognito": {
        "ConfirmedUserSeedScriptPath": "../../../../scripts/development/get_confirmed_user_count.sh"
    }
}

